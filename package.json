{
  "name": "escalade-dealer-locator",
  "version": "0.0.0",
  "main": "dist/index.js",
  "browser": "dist-browser/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/escalade-dealer-locator.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "esca-scripts analyze --input './src/inject.js' --react --browser",
	 "build": "run-p build:main build:browser",
	 "build:main": "esca-scripts build --input './src/index.js' --output './dist/index.js' --react",
	 "build:browser": "esca-scripts build --input './src/inject.js' --output './dist-browser/index.js' --react --browser",
    "dev": "esca-scripts dev ---input './src/inject.js, ./dev/index.html' --browser --react",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "esca-scripts": "^0.9.2",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {
    "google-map-react": "^0.29.0",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "zippo": "^1.0.1"
  }
}
